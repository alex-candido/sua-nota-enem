  },
  "scripts": {
    "build": "bun i && nest build && cp -R views dist/views",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "repl": "nest start -- --entryFile repl",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:init": "jest --init ",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:e2e:watch": "jest --config ./test/jest-e2e.config.ts --watch",
    "prisma:generate": "dotenv -e ./envs/.env -- prisma generate --schema ./prisma/schema.prisma",
    "prisma:push": "dotenv -e ./envs/.env -- prisma db push --schema ./prisma/schema.prisma",
    "prisma:migrate": "dotenv -e ./envs/.env -- prisma migrate dev --schema ./prisma/schema.prisma",
    "prisma:studio": "dotenv -e ./envs/.env -- prisma studio --schema ./prisma/schema.prisma",
    "tailwind:watch": "postcss assets/stylesheets/application.tailwind.css -o public/css/tailwind-main.css --watch",
    "tailwind:build": "postcss assets/stylesheets/application.tailwind.css -o public/css/tailwind-main.css",
    "sass:watch": "sass --watch assets/stylesheets/application.scss public/css/sass-main.css",
    "build:ts": "bun build assets/typescript/main.ts --outdir public/js/ --watch",
    "build:js": "rollup -c --bundleConfigAsCjs rollup.config.js"
  },
